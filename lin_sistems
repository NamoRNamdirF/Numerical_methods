    "### Задание 2.\n",
    "Составить программу для вычисления определителяn-го порядка."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 95,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5800.215680000002\n"
     ]
    }
   ],
   "source": [
    "def minor(m, i, j):\n",
    "    tmp = [row for k, row in enumerate(m) if k != i]\n",
    "    tmp = [col for k, col in enumerate(zip(*tmp)) if k != j]\n",
    "    return tmp\n",
    "\n",
    "def det2(matrix):\n",
    "    return matrix[0][0] * matrix[1][1] - matrix[0][1] * matrix[1][0]\n",
    "\n",
    "def determinant(matrix):\n",
    "    size = len(matrix)\n",
    "    if size == 2:\n",
    "        return det2(matrix)\n",
    "\n",
    "    return sum((-1) ** j * matrix[0][j] * determinant(minor(matrix, 0, j))\n",
    "               for j in range(size))\n",
    "m = [[14.38, -2.41, 1.39],\n",
    "       [1.84, 25.36, -3.31],\n",
    "       [2.46, -3.49, 16.37],]\n",
    "print (determinant(m))\n"
   ]
  },
